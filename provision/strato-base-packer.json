{
	"builders": [
		{
			"type": "docker",
			"image": "ubuntu:12.04",
			"export_path": "strato-base.image.tar"
		}
	],
	"provisioners": [
		{
			"type": "shell",
			"script": "provision.sh"
		}
	],
	"post-processors": [
		{
			"type": "docker-import",
			"repository": "strato/strato-base",
			"tag": "latest"
		}
	]
}
